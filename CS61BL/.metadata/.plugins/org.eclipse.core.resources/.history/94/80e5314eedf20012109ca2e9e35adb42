public class BinarySearchTree extends BinaryTree {

	public boolean contains(Comparable key) {
		return containsHelper(Comparable key, this);
	}

	public boolean containsHelper(Comparable key, BinarySearchTree t) {
		if (myRoot == null) {
			return false;
		} else if (key.compareTo(myRoot.myItem) == 0) {
			return true;
		} else if (key.compareTo(myRoot.myItem) < 0) {
			return containsHelper(key, myRoot.myLeft);
		} else {
			return containsHelper(key, myRoot.myRight);
		}
	}

	public void add() {

	}

}