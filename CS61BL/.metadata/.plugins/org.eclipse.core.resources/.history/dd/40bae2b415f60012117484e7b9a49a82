import java.util.HashMap;

public class Fibonacci {
	int callsToFib;
	int result;
	HashMap alreadySeen = new HashMap();

	public Fibonacci(int n) {
		return fibHelper (n, alreadySeen);
	}

	private int fib(int n) {
		callsToFib++;
		if (n == 0) {
			return 0;
		} else if (n == 1) {
			return 1;
		} else {
			int returnValue = fib(n - 1) + fib(n - 2);
			return returnValue;
		}
	}
}
