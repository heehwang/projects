import java.util.HashMap;

public class Fibonacci {
	int callsToFib;
	int result;
	HashMap alreadySeen = new HashMap();

	public Fibonacci(int n) {
		this.callsToFib = 0;
		this.result = fib(n);
	}
	
	public int fib(int n){

	private int fibHelper(int n, HashMap alreadySeen) {
		callsToFib++;
		if (n == 0) {
			return 0;
		} else if (n == 1) {
			return 1;
		} else {
			int returnValue = fib(n - 1) + fib(n - 2);
			return returnValue;
		}
	}
}
